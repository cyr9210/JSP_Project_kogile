<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<typeAliases>
		<typeAlias type="kogile.module.ExterMember" alias="ExterMember"/> 
		<typeAlias type="kogile.module.InterMember" alias="InterMember"/> 
		<typeAlias type="kogile.module.IsInterMem" alias="IsInterMem"/> 
		<typeAlias type="kogile.module.TotalMemInfo" alias="TotalMemInfo"/> 
	</typeAliases>

	<environments default="development">
		<environment id="development">
			<transactionManager type="JDBC" />
			<dataSource type="POOLED">
				<property name="driver"
					value="oracle.jdbc.driver.OracleDriver" />
				<property name="url"
					value="jdbc:oracle:thin:@localhost:1521:XE" />
				<property name="username" value="kogile" />
				<property name="password" value="1234" />
			</dataSource>
		</environment>
	</environments>

	<mappers>
		<mapper resource="kogile/interMem/Mapper/InterMem.xml" />
		<mapper resource="kogile/kakaoOauth/Mapper/kakaoOauth.xml"/>
	</mappers>

</configuration>
