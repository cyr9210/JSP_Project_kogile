<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="kogile.startPage.Mapper.PbMapper">
	<cache />

	<insert id="insertBoard" parameterType="ProjectBoard">
		insert into	PROJECT values(pjt_no.nextval , sysdate , #{pjt_title},  #{pjt_writer} , #{pjt_contents}  )
	</insert>
	
	<select id="projectList" resultType="ProjectBoard">
		select * from PROJECT
	</select>
	
	<select id="mainDetail" resultType="ProjectBoard" parameterType="int">
		select * from PROJECT where pjt_no = #{seq}
	</select>
</mapper>















